                    }
                ],
                "optional": [
                    {
                        "name": "oplock_enabled",
                        "field_type": "select",
                        "allowed_values": ["yes", "no"],
                        "default": "yes"
                    },
                    {
                        "name": "notify_enabled",
                        "field_type": "select",
                        "allowed_values": ["yes", "no"],
                        "default": "yes"
                    },
                    {
                        "name": "offline_file_mode",
                        "field_type": "select",
