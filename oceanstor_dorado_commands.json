                    },
                    {
                        "name": "user_group_type",
                        "field_type": "select",
                        "allowed_values": ["local", "domain"],
                        "default": "local"
                    },
                    {
                        "name": "domain_type",
                        "field_type": "select",
                        "allowed_values": ["local", "ad", "ldap", "nis"],
                        "default": "local"
                    },
                    {
                        "name": "vstore_id",
                        "field_type": "text",
                        "default": "0"
                    }
                ]
