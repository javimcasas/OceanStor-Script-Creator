                        "default": "no"
                    },
                    {
                        "name": "abe_enabled",
                        "field_type": "select",
                        "allowed_values": ["yes", "no"],
                        "default": "no"
                    },
                    {
                        "name": "apply_default_acl",
                        "field_type": "select",
                        "allowed_values": ["yes", "no"],
                        "default": "yes"
                    },
                    {
                        "name": "show_previous_versions_enabled",
                        "field_type": "select",
