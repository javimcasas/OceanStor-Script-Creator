                    },
                    {
                        "name": "dtree_id",
                        "field_type": "text"
                    },
                    {
                        "name": "dtree_name",
                        "field_type": "text"
                    },
                    {
                        "name": "quota_type",
                        "field_type": "select",
                        "allowed_values": ["directory", "user", "group"]
                    },
                    {
                        "name": "vstore_id",
                        "field_type": "text"
                    }
                ]
