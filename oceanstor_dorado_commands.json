                        "default": "yes"
                    },
                    {
                        "name": "show_snapshot_enabled",
                        "field_type": "select",
                        "allowed_values": ["yes", "no"],
                        "default": "yes"
                    },
                    {
                        "name": "browse_enabled",
                        "field_type": "select",
                        "allowed_values": ["yes", "no"],
                        "default": "yes"
                    },
                    {
                        "name": "file_system_name",
