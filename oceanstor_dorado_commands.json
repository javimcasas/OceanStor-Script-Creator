    "CIFS": {
        "operations": {
            "Create": {
                "cli_prefix": "create share cifs",
                "mandatory": [
                    {
                        "name": "name",
                        "field_type": "text"
                    },
                    {
                        "name": "local_path",
                        "field_type": "text",
                        "transform": "prefix_slash"
                    }
                ],
                "optional": [
                    {
                        "name": "oplock_enabled",
