            "Change": {
                "cli_prefix": "change quota",
                "mandatory": [
                    {
                        "name": "quota_id",
                        "field_type": "text"
                    },
                    {
                        "name": "space_hard_quota",
                        "field_type": "text"
                    },
                    {
                        "name": "space_soft_quota",
                        "field_type": "text"
                    },
                    {
                        "name": "file_hard_quota",
                        "field_type": "text"
                    },
                    {
                        "name": "file_soft_quota",
                        "field_type": "text"
                    }
                ],
                "optional": []
            },
            "Show": {
                "cli_prefix": "show quota",
                "mandatory": [],
                "optional": [
                    {
                        "name": "quota_id",
