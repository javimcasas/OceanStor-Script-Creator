    "Quota": {
        "operations": {
            "FS Quota": {
                "cli_prefix": "create file_system quota",
                "mandatory": [
                    {
                        "name": "file_system_name",
                        "field_type": "text"
                    },
                    {
                        "name": "quota_type",
                        "field_type": "select",
                        "allowed_values": ["directory", "user", "group"],
                        "default": "directory"
                    },
                    {
                        "name": "space_hard_quota",
                        "field_type": "text"
                    },
                    {
                        "name": "space_soft_quota",
                        "field_type": "text"
                    },
                    {
                        "name": "file_hard_quota",
                        "field_type": "text"
                    },
                    {
                        "name": "file_soft_quota",
