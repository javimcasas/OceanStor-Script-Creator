    "NFS": {
        "operations": {
            "Create": {
                "cli_prefix": "create share nfs",
                "mandatory": [
                    {
                        "name": "local_path",
                        "field_type": "text",
                        "transform": "prefix_slash"
                    }
                ],
                "optional": [
                    {
                        "name": "file_system_id",
                        "field_type": "text"
                    },
                    {
                        "name": "charset",
